{{> header}}
<h1>Altere sua senha.</h1>

<form method="post">
  <label>
    Nova Senha:
    <input type="password" name="password" />
  </label>
  <br />
  <label>
    Confirmar Nova Senha:
    <input type="password" name="passwordConfirm" />
  </label>
  <br />
  <input type="submit" value="Salvar" />
</form>
